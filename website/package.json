  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm run type-check",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --pretty --noEmit",
    "ghpages": "gatsby build --prefix-paths && gh-pages -d public",
    "start:css": "less-watch-compiler --config less.json",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "remove:css": "find src -type f -name '*.css' -delete",
    "deploy": "gatsby build && scp -r public root@smestad.xyz:/var/www/cabin/",
    "dev": "concurrently --kill-others \"npm run develop \" \"npm run start:css\""
